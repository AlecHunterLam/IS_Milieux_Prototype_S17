<section id="contact" style="">
            <div class="container">
                <div class="row">
                    <div class="about_our_company" style="margin-bottom: 20px;">
                        <h1 class="text-center">Contact Us</h1>
                        <div class="titleline-icon"></div>
                        <p class="text-center">Lorem Ipsum is simply dummy text of the printing and typesetting </p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-8">
                        <form name="sentMessage" id="contactForm" onsubmit="return validate()">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <p>Name:</p>
                                        <input type="text" class="form-control" placeholder="John Doe" id="name" required data-validation-required-message="Please enter your name.">
                                        <p class="help-block text-danger"></p>
                                    </div>
                                    <div class="form-group">
                                        <p>Email Address:</p>
                                        <input type="email" class="form-control" placeholder="example@gmail.com" id="email" required data-validation-required-message="Please enter your email address.">
                                        <p class="help-block text-danger"></p>
                                    </div>
                                    <div class="form-group">
                                        <p>Phone Number:</p>
                                        <input type="tel" class="form-control" placeholder="4121231234" id="phoneTxt" required data-validation-required-message="Please enter your phone number.">
                                        <p class="help-block text-danger"></p>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <p>Message:</p>
                                    <div class="form-group">
                                        <textarea rows="9" class="form-control" placeholder="I wish Big Skinny offered..." id="message" required data-validation-required-message="Please enter a message."></textarea>
                                        <p class="help-block text-danger"></p>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="col-lg-12 text-center">
                                    <div id="success"></div>
                                    <button type="submit" class="btn btn-xl get">Send Message</button>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-md-4">
                        <p>
                            <strong><i class="fa fa-map-marker"></i> Address:</strong><br>
                            1151 Massachusetts Ave <br/>Cambridge, MA 02138
                        </p>
                        <p><strong><i class="fa fa-phone"></i> Phone Number:</strong><br>
                            617-500-6939</p>
                        <p>
                            <strong><i class="fa fa-envelope"></i>  Email Address:</strong><br>
                             service@bigskinny.com</p>
                        <p></p>
                        <div id="copy">
                        	<%= link_to(image_tag('facebook.png', class: "img-responsive inline-block social"), "https://www.facebook.com/bigskinny/", target: "_blank") %>
                        	<%= link_to(image_tag('twitter.png', class: "img-responsive inlineblock social"), "https://twitter.com/big_skinny?lang=en", target: "_blank") %>
                        	<%= link_to(image_tag('instagram.png', class: "img-responsive inline-block social"), "https://www.instagram.com/bigskinnywallets/?hl=en", target: "_blank") %>
                        </div>
                    </div>
                </div>
            </div>
            <br/>
        </section>

